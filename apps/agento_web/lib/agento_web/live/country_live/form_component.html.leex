<h2><%= @title %></h2>

<%= f = form_for @changeset, "#",
  id: "country-form",
  phx_target: @myself,
  phx_change: "validate",
  phx_submit: "save" %>

  <%= label f, :alpha2 %>
  <%= text_input f, :alpha2 %>
  <%= error_tag f, :alpha2 %>

  <%= label f, :alpha3 %>
  <%= text_input f, :alpha3 %>
  <%= error_tag f, :alpha3 %>

  <%= label f, :numeric3 %>
  <%= text_input f, :numeric3 %>
  <%= error_tag f, :numeric3 %>

  <%= label f, :name %>
  <%= text_input f, :name %>
  <%= error_tag f, :name %>

  <%= submit "Save", phx_disable_with: "Saving..." %>
</form>
